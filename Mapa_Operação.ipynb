    "#Calculo das medias de latitude  e de longitude para inicialização do zoom do mapa\n",
    "media_latitude = gpd_carteira['Latitude'].mean()\n",
    "media_longitude = gpd_carteira['Longitude'].mean()\n",
    "#Criação do mapa base\n",
    "m = folium.Map(location =[media_latitude, media_longitude], zoom_start=4, tiles=None)\n",
    "folium.raster_layers.TileLayer('OpenStreetMap', name='Open Street Map').add_to(m)\n",
    "#folium.raster_layers.TileLayer('cartodbdark_matter', name='Tema Escuro').add_to(m)\n",
    "#ferramenta de busca\n",
    "#folium.plugins.Search(gpd_carteira, search_zoom=20).add_to(m)\n",
    "#Ferramenta de medição\n",
    "m.add_child(MeasureControl())\n",
    "#ferramenta de busca pdv\n",
    "credenciados_folium = folium.GeoJson(gpd_carteira,show=False,name='Todos PDVs').add_to(m)\n",
    "credenciados_search = Search(layer=credenciados_folium,geom_type='Point',placeholder='Credenciados',collapsed=False,search_label='PDV').add_to(m)\n",
    "#Ferramenta MarkerCluster com grupo e subgrupo\n",
    "marker = folium.plugins.MarkerCluster(control=False)\n",
    "m.add_child(marker)\n",
    "especial = folium.plugins.FeatureGroupSubGroup(marker, 'Especiais')\n",
    "m.add_child(especial)\n",
    "especialm = folium.plugins.FeatureGroupSubGroup(marker, 'Especiais Mais')\n",
    "m.add_child(especialm)\n",
    "sky = folium.plugins.FeatureGroupSubGroup(marker, 'Sky')\n",
    "m.add_child(sky)\n",
    "#Ferramenta de desenho\n",
    "draw = Draw()\n",
    "draw.add_to(m)\n",
    "#Ferramenta de tela cheia\n",
    "Fullscreen(position = 'topright', title = 'Tela Cheia', title_cancel = 'Sair da Tela Cheia', force_separate_button = True).add_to(m)\n",
    "#adicionando municipios\n",
    "folium.GeoJson(municipios,name='Municípios').add_to(m)\n",
    "#adicionando os pontos\n",
    "for i in range(0,len(gpd_especiais)):\n",
    "    html = html_especiais(i) \n",
    "    iframe = branca.element.IFrame(html=html,width=400,height=300)\n",
    "    popup = folium.Popup(iframe,parse_html=True)    \n",
    "    folium.Marker([gpd_especiais['Latitude'].iloc[i],\n",
    "    gpd_especiais['Longitude'].iloc[i]],\n",
    "    popup=popup,\n",
    "    icon=folium.Icon(color='orange', icon='star'),\n",
    "    tooltip=\"Clique aqui\").add_to(especial)\n",
    "\n",
    "for i in range(0,len(gpd_especiaism)):\n",
    "    html = html_especiaism(i) \n",
    "    iframe = branca.element.IFrame(html=html,width=400,height=300)\n",
    "    popup = folium.Popup(iframe,parse_html=True)    \n",
    "    folium.Marker([gpd_especiaism['Latitude'].iloc[i],\n",
    "    gpd_especiaism['Longitude'].iloc[i]],\n",
    "    popup=popup,\n",
    "    icon=folium.Icon(color='red', icon='star'),\n",
    "    tooltip=\"Clique aqui\").add_to(especialm)\n",
    "\n",
    "for i in range(0,len(gpd_sky)):\n",
    "    html = html_sky(i) \n",
    "    iframe = branca.element.IFrame(html=html,width=400,height=300)\n",
    "    popup = folium.Popup(iframe,parse_html=True)    \n",
    "    folium.Marker([gpd_sky['Latitude'].iloc[i],\n",
